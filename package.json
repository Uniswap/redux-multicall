    "prepublishOnly": "tsdx build",
    "contracts:compile": "typechain --target ethers-v5 --out-dir src/abi/types './node_modules/@uniswap/v3-periphery/artifacts/contracts/**/*Multicall*.json'"
  },
  "dependencies": {
    "@uniswap/v3-periphery": "^1.2.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@typechain/ethers-v5": "^7.2.0",
    "@types/jest": "^24.0.25",
    "@types/react": "^17.0.28",
    "ethers": "^5.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "tsdx": "^0.14.1",
    "typechain": "^5.2.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "1.x",
    "ethers": "5.x",
    "react": "^17.x",
    "react-redux": "^7.x"
  },
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  }
